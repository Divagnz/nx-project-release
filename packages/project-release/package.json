{
  "name": "nx-project-release",
  "version": "0.0.33",
  "description": "Polyglot Nx plugin for releasing any project type using project.json and conventional commits",
  "keywords": [
    "nx",
    "plugin",
    "release",
    "semver",
    "conventional-commits"
  ],
  "author": "Divagnz",
  "license": "MIT",
  "type": "module",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "executors": "./executors.json",
  "generators": "./generators.json",
  "nx-migrations": {
    "packageJsonUpdates": {}
  },
  "nx-json-plugins": {
    "projectRelease": "./src/nx-json-schema.json"
  },
  "scripts": {
    "postinstall": "node scripts/postinstall.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Divagnz/nx-project-release.git"
  },
  "bugs": {
    "url": "https://github.com/Divagnz/nx-project-release/issues"
  },
  "homepage": "https://github.com/Divagnz/nx-project-release#readme",
  "engines": {
    "node": ">=20.0.0"
  },
  "peerDependencies": {
    "@nx/devkit": ">=20.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "enquirer": "^2.3.6",
    "glob": "^10.5.0",
    "minimatch": "^9.0.3",
    "rxjs": "^7.8.1",
    "semver": "^7.7.2",
    "tslib": "^2.8.1"
  },
  "optionalDependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@types/archiver": "^7.0.0",
    "@types/tar": "^6.1.13",
    "archiver": "^7.0.1",
    "tar": "^7.5.2"
  },
  "overrides": {
    "js-yaml": "^4.1.0",
    "glob": "^13.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/config-nx-scopes": "^20.0.0"
  }
}
